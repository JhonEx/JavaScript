function findGreaterOrEqual(a, b) {
  if(a === b) {
    return "a and b are equal";
  }
  else if(a > b) {
    return "a is greater";
  }
  else {
    return "b is greater";
  }
}


//// Another example:
function checkSign(num) {
      return (num < 0) ?"negative" : (num > 0) ?"positive" : "zero";
}
console.log(checkSign(0));

